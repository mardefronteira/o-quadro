{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, {"extensions": [".js"] } ],
      "linebreak-style": "off",
      "prefer-destructuring": [
        "error",
        {
            "object": true,
            "array": false
        }
    ]
    }
}
